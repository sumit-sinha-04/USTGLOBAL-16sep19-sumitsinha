package com.ustglobaldi;

public class Hello {

	
	private String msg;

	public String getMsg() {
		return msg;
	}

	public void setMsg(String msg) {
		this.msg = msg;
	}
public void init()
{
System.out.println("init method executing from hello");
}
	
public void destroy()
{
System.out.println("Destroy method executing from hello");	
}
	
	
	
	
	
	
	
	
}
