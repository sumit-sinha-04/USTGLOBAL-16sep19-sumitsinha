package com.ustglobal.pausing;

public class Slider {

	public static void main(String[] args) {
		
		
		System.out.println("main started");
		
		for (int i = 0; i < 5; i++) {
		
			Thread.sleep(2000);
			System.out.println("sliding");
		}
		
		try {
			
		
			
			
			
		} catch (Exception e) {
			e.printStackTrace();
		}
		
	}
}
