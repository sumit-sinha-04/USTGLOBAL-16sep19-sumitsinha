package com.ustglobal.pausing;

public class Test {

	public static void main(String[] args) {
		
		Counter c = new Counter();
		
		Runnable r1 =()->
		{
			
			c.increment();
			c.decrement();
			
			
		};
		
		Runnable r2 = ()->{
			
			c.decrement();
			c.increment();
		
		};
		
		Thread t1 = new Thread(r1);
		Thread t2 = new Thread(r2);
		
		
		
		
	}
}
