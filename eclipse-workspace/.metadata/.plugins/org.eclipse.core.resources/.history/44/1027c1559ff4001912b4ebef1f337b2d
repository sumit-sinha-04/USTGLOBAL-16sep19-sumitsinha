
public class Employee {
String ename;
int eid;
public  Employee(String empname , String empid ) {
	ename=empname;
	eid=empid;
}
public static void main(String[] args) {
	Employee e1 = new Employee("rashmika",143);
	e1.ename ="rashmika";
	e1.eid=143;
	Employee e2 = new Employee();
	e2.ename="manasi";
	e2.eid= 143;
	Employee e3 = new  Employee();
	e3.ename= "chacha";
	e3.eid= 69;
	
}
}
